# Instagram Downloader + MP3 + Transcrição
# Configuração de ambiente

# Porta do servidor (padrão: 5000)
PORT=5000

# Modo de desenvolvimento (true/false)
DEBUG=true

# Limite de tamanho de arquivo em MB (padrão: 100)
MAX_FILE_SIZE=100

# Tempo de limpeza de arquivos em minutos (padrão: 60)
CLEANUP_INTERVAL=60

# Qualidade padrão MP3 (128, 192, 320)
DEFAULT_MP3_QUALITY=192

# Idioma padrão para transcrição (pt, en, es, fr, de, it)
DEFAULT_LANGUAGE=pt
